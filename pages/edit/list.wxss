.menu_icon{color: #000;}
.bts{float: left;width: 100%;padding:10px;}
.bts view{float: left;padding-right: 15px;padding-left: 15px;height: 32px;line-height: 32px;background: #09c;color: #fff;border-radius: 5px;margin-left: 10px;}

.items{float: left;width: 100%;padding-right: 10px;padding-left:110px;margin-top: 5px;margin-bottom: 5px;position: relative;border-bottom: 1px solid #eee;padding-bottom: 10px;}
.items image{position: absolute;width: 100px;left: 0px;border-radius: 5px;}
.box{float: left;width: 100%;padding: 10px;padding-bottom: 50px;}
.items .l{float: left;width: 100%;padding-left: 50px;line-height: 32px;position: relative;}
.items .l text{position: absolute;width: 50px;left: 0;height: 32px;line-height: 32px;}
.items .l input{float: left;width: 100%;height: 32px;line-height: 32px;}
.items .l textarea{float: left;width: 100%;max-height: 60px;line-height: 20px;}
.items .l .icon-nav{position: absolute;right: 0;color: #f44;z-index: 100;font-size: 18px;}
.items .l .icon-jinpai{position: absolute;right: 0;color: #f44;z-index: 100;font-size: 18px;}
.win_c_show{bottom: 0;height: auto;padding-bottom: 30px;}
.item_c{float: left;height: 32px;line-height: 32px;margin-right: 10px;margin-bottom: 5px;padding-left: 15px;padding-right: 15px;}

.items .edit_icon{float: right;font-size: 20px;margin-left: 10px;color: #f44;}
.start_bt{position: absolute;width: 80px;height:30px;line-height: 30px;text-align: center;left: 10px;bottom: 5px;background: #09c;color: #fff;border-radius: 5px;}
.is_stop{color: #fff;background: #f44;}
.cy_ts{position: absolute;width: 80px;height: 30px;line-height: 30px;text-align: center;left: 10px;bottom: 35px;}
.win{height: auto;}
.cy_ts text{font-size: 20px;color: #ccc;margin-left: 5px;margin-right: 5px;}
.cy_ts .is_select{color: #f44;}

.handle .bt{position: absolute;right:40px;height: 32px;line-height: 32px;background: gold;color: #000;padding-left: 15px;padding-right: 15px;border-radius: 5px;width: auto;top: 5px;}

.style_box .win_box{height: 60vh;}
.item_s{float: left;height: 32px;line-height: 32px;margin-top: 5px;width: 100%;border-bottom: 1px solid #eee;margin-bottom: 5px;}
.item_s .bt{float: right;height: 32px;line-height: 32px;background: gold;color: #000;padding-left: 15px;padding-right: 15px;border-radius: 5px;width: auto;}







