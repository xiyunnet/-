page{background: #fed351;}
.main{float: left;width: 100%;}

.default_mode{float: left;width: 100%;padding-left: 10px;padding-right: 10px;padding-bottom:40px;}

.default_mode .ll{float: left;width:50%;padding:4px;}
.default_mode .l{float: left;width: 100%;height:56px;color: #000;font-size: 20px;position: relative;border-radius: 5px;padding-right:5px;padding-left: 5px;}
.default_mode .l .title{float: left;width: 100%;overflow: hidden;height: 25px;line-height: 25px;padding-left: 5px;font-weight: bold;margin-top: 3px;}
.default_mode .l .iconfont{height:24px;line-height: 24px;font-size: 26px;float: right;font-weight: normal;}
.default_mode .l .is_select{color: #f44;}
.default_mode .select{background: #000;color: #fff !important;}
.default_mode .l .price{float: left;width: 100%;position: relative;font-weight: bold;height: 22px;line-height: 22px;}
.default_mode .l .price text{font-size: 14px;font-weight: normal;}
.default_mode .c{float: left;width: 100%;height: 30px;line-height: 30px;font-weight: bold;font-size:22px;padding-left: 10px;padding-right: 10px;}
.default_mode .lx{float: left;width: 100%;}
.default_mode .select text,.default_mode .select view{color: #fff;}


.cart{position: fixed;height: 60px;border-radius: 30px;background: #09c;color: #fff;font-size: 30px;line-height: 60px;right: 15px;bottom:35px;z-index: 10000;text-align: center;width: 180px;padding-top:10px;}
.cart .bt{float: right;width: 60px;background: #fff;color: #000;border-radius: 50%;font-size: 30px;position: absolute;right: 0;top: 0;}
.cart .info{float: left;width: 120px;height: 20px;line-height: 20px;overflow: hidden;font-size: 15px;padding-left:15px;text-align: left;}



.new_mode{float: left;width: 100%;margin-top: 100px;background: #eee;position: relative;}
.top_adv{float: left;width: 100%;height: 200px;margin-top: -100px;padding: 10px;padding-left: 20px;padding-right: 20px;}
.top_adv swiper{float: left;width: 100%;height: 180px;border-radius: 10px;overflow: hidden;}
.top_adv image{float: left;width: 100%;border-radius: 10px;}

.new_mode .items{float: left;width: 100%;padding-left: 80px;position: relative;}
.new_mode .items .left{width: 80px;height: 80vh;background: rgba(255,255,255,0.4);position: absolute;left: 0;text-align: center;padding-bottom: 10px;}
.new_mode .items .left .item{float: left;width: 100%;position: relative;}
.new_mode .items  .left text{position: absolute;width: 20px;height: 20px;line-height: 20px;background: #000;color: #fed351;border-radius: 50%;right:-10px;top:-7px;text-align: center;font-size: 12px;}
.new_mode .items .left .item .text{float: left;width:40px;line-height: 25px;margin-top: 20px;color: #999;text-align: center;margin-left:20px ;margin-bottom: 20px;}


.new_mode .items .left .is_select{background: #eee;box-shadow:3px 0px 0px 0 #fed351 inset;color: #000 !important;}
.new_mode .items .left .is_select .text{color: #333;font-weight: bold;}
.new_mode .items .right{float: right;width: 100%;padding-left: 15px;padding-right: 20px;height:80vh;}
.have_cart{padding-bottom:80px;}
.new_mode .items .right .cc{float: left;width: 100%;font-size: 16px;color: #666;height: 32px;line-height: 28px;}

.tj_item{float: left;width:100%;height: 280px;background: #fff;border-radius: 10px;overflow: hidden;margin-bottom:20px ;}
.tj_item image{float: left;width: 100%;height: 180px;}
.tj_item .title{float: left;width: 100%;height: 50px;line-height: 50px;overflow: hidden;font-weight: bold;padding-right: 10px;padding-left: 10px;font-size: 18px;}
.tj_item .price{float: left;width: 100%;font-size: 26px;font-weight: bold;margin-left: 10px;}
.tj_item .price text{font-size: 16px;}
.tj_item .jiao{position: absolute;font-size: 40px;right: 0;color: #fed351;top:-2px}
.tj_item .icon-shoucangjiaobiao{top:-5px}

.go_buy{position: absolute;right: 10px;height: 50px;line-height: 50px;bottom:-8px;}
.go_buy .iconfont{float: right;width: 28px;line-height: 28px;text-align: center;border-radius: 50%;background: #fed351;color: #000;font-size: 20px;font-weight: bold;height: 28px;}
.go_buy .icon-jian{color: #999;border: 1px solid #999;background: #fff;width: 26px;line-height:26px;height: 26px;font-weight: bold;}
.go_buy .num{float: right;text-align: center;height: 30px;line-height: 30px;font-size: 18px;padding-left: 8px;padding-right: 8px;}
.go_buy .buy_bt{float: right;height: 32px;line-height: 32px;padding-right: 10px;padding-left: 10px;background: #fed351;color: #000;border-radius: 16px;font-size: 14px;}

.dft_item{float: left;width:100%;height:120px;background: #fff;border-radius: 10px;overflow: hidden;margin-bottom:20px;padding-left:120px;position: relative;}
.dft_item image{position: absolute;width: 120px;height: 120px;left: 0;top: 0;}
.dft_item .title{float: left;width: 100%;height: 70px;line-height: 70px;overflow: hidden;font-weight: bold;padding-right: 10px;padding-left: 10px;font-size: 18px;overflow: hidden;width: 100%;padding-right: 10px;}
.dft_item .price{float: left;width: 100%;font-size: 26px;font-weight: bold;margin-left: 10px;}
.dft_item .price text{font-size: 16px;}



.show_cart{position: fixed;width: 90%;height: 56px;margin-left:5% ;background: #333;border-radius: 28px;bottom:-100px;z-index: 1000;color: #fff;padding-left: 56px;}
.iconbox{width: 46px;height: 46px;position: absolute;border-radius: 50%;text-align: center;line-height: 46px;left:5px;background:rgba(255,255,255,0.2);top: 5px;}
.iconbox .iconfont{font-size:26px;}
.iconbox .num{position: absolute;width: 18px;height: 18px;background: #fed351;color: #000;text-align: center;line-height: 18px;font-size: 12px;border-radius: 50%;right:-5px;}
.show_cart .buy_bt{height: 46px;line-height: 46px;background: #fed351;border-radius: 23px;color: #000;padding-right: 20px;padding-left: 20px;font-weight: bold;font-size: 18px;position: absolute;right: 5px;top:5px}

.show_cart .price{float: left;height: 32px;line-height: 32px;width: 100%;font-size: 26px;margin-top: 3px;padding-left: 8px;font-weight: bold;}
.show_cart .price text{font-size: 16px;font-weight: normal;}
.show_cart .info{float: left;height: 18px;line-height: 18px;color: #999;font-size: 12px;margin-left: 8px;}

.cart_show{bottom: 20px;}
::-webkit-scrollbar{width: 0;height: 0;color: transparent;}


.win_box{width: 100%;transform: translate(0, -40%);padding-right: 20px;padding-left: 20px;z-index: 100000;top: 40%;display: none;position: fixed;}
.win_box_show{display: block;}
.win_box .box{float: left;width: 100%;padding: 10px;border-radius: 10px;background: #fff;}
.win_box .box .handle{float: left;width: 100%;font-weight: bold;font-size: 18px;height:32px;line-height: 20px;}
.win_box .close_bt{position: absolute;width: 28px;height: 28px;border: 2px solid #999;color: #999;text-align: center;line-height:24px !important;right: 10px;top: 5px;font-size: 20px;border-radius: 50%;}
.scroll_box{float: left;width: 100%;min-height: 20vh;max-height:50vh;}

.style_c_t{float: left;width: 100%;height: 32px;line-height: 32px;font-size: 16px;}
.style_bt{float: left;width: 33.33%;height: 32px;line-height: 32px;padding-left: 6px;padding-right: 6px;margin-bottom:10px;}
.style_bt text{float: left;width: 100%;background: #eee;border-radius: 10px;overflow: hidden;height: 32px;line-height: 32px;padding-right: 3px;padding-left: 3px;border-radius: 4px;text-align: center;}
.is_select_bt{float: left;width: 96%;height: 42px;line-height: 42px;background:#fed351;color: #000;font-size: 16px;text-align: center;font-weight: bold;border-radius: 5px;margin-top: 5px;margin-left: 2%;}


.style_select_info{float: left;width: 100%;}
.style_select_info .price{float: left;width: 100%;font-size: 18px;}
.style_select_info .price text{font-size: 14px;}
.style_select_info .info{float: left;width: 100%;height: 25px;line-height: 25px;font-size: 14px;}

.scroll_box .is_select{background: #fed351;color: #000;}
.buy_bt text{position: absolute;width: 14px;height: 14px;border-radius: 50%;top:-3px;right:2px;line-height: 14px;text-align:center;font-size: 11px;background: #000;color: #fed351;}

.select_win{position: fixed;width: 100%;height: 75vh;background: #fff;border-top-left-radius:20px;border-top-right-radius:20px;bottom: 0;left: 0;z-index: 100;bottom: -100vh;}
.select_win scroll-view{float: left;width: 100%;min-height:20vh;height:65vh;padding-right: 15px;padding-left: 15px;padding-bottom:70px;padding-top: 10px;}
.select_handle{float: left;width: 100%;height: 32px;line-height: 32px;margin-top: 10px;}
.select_handle .tt{float: left;margin-left: 15px;font-size: 18px;font-weight: bold;}
.select_handle .iconfont{float: right;color: #666;font-size:16px;margin-right: 15px;}
.s_item{float: left;width: 100%;height:90px;padding-top:8px;padding-bottom:8px;padding-left: 90px;}
.s_item image{height: 80px;width: 80px;position:absolute;left: 0;top: 0;border-radius:8px;}
.s_item .title{float: left;width: 100%;height: 24px;line-height: 24px;overflow: hidden;font-weight: bold;}
.s_item .info{float: left;width: 100%;height: 16px;line-height: 16px;color: #f44;font-size: 14px;overflow: hidden;}
.s_item .price{float:left;width: 100%;height: 30px;line-height: 30px;font-weight: bold;}

.s_item .num{float:right;height: 30px;line-height: 30px;}

.s_item .num .iconfont{float: right;width: 24px;height: 24px;line-height: 24px;text-align:center;background: #fed351;border-radius: 50%;margin-top:3px ;}
.s_item .num .icon-jian{color: #999;border: 1px solid #999;background: none;width: 22px;line-height:22px;height: 22px;}
.s_item .num text{float: right;font-weight: normal;text-align: center;height: 30px;line-height:30px;margin-right:10px;margin-left:10px;}
.select_win_show{bottom: 0;}



