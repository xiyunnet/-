.main{float: left;width: 100%;padding: 10px;}
.list{float: left;width: 100%;padding: 10px;margin-bottom: 10px;}
.list .i{float: left;width: 100%;height: 25px;line-height: 20px;color: #999;font-size: 14px;border-bottom: 1px solid #eee;}
.list .i text{float: right;}

.item{float: left;width: 100%;height: 80px;padding-left: 90px;position:relative;margin-top:15px;}
.item image{position: absolute;width: 80px;height: 80px;left: 0;top: 0;border-radius: 5px;}
.item  .title{float: left;width: 100%;height: 20px;line-height: 20px;font-weight: bold;}
.item .price{float: left;width: 100%;height: 30px;line-height: 30px;}
.item .price text{float:right;}
.item .info{float: left;width: 100%;color: #f44;font-size: 14px;}

.total{float: left;width: 100%;text-align: right;height: 32px;line-height: 32px;}
.total text{font-weight: bold;font-size: 20px;}
.o_info{float: left;color: #f44;height: 32px;line-height: 36px;}
.bt{float: left;width: 100%;height: 32px;margin-top: 5px;}
.bt view{float: right;padding-left: 10px;padding-right: 10px;margin-left: 10px;height: 32px;line-height: 32px;border-radius: 5px;color:#fff;background: #09c;}


